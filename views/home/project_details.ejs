        function tell_to_friend() {

            var emailRegex = /^[A-Za-z0-9._]*\@[A-Za-z]*\.[A-Za-z]{2,5}$/;

            var filter = /^[0-9-+]+$/;

            var name = $("#your_name").val();

            var f_name = $("#friend_name").val();

            var your_email = $("#your_email").val();

            var email = $("#friend_email").val();

            var id = '<%= project_id %>'

            if (name == "") {

                $("#your_name").focus();

                $("#errorBox").html("<div class='alert alert-error fade in'> <a href='#' data-dismiss='alert' class='close'>x</a> <strong>Please Enter Your Name</strong></div>");


            } else if (your_email == "") {

                $("#your_email").focus();

                $("#errorBox").html("<div class='alert alert-error fade in'> <a href='#' data-dismiss='alert' class='close'>x</a> <strong>Please Enter Your Valid Email Id</strong></div>");


            } else if (!emailRegex.test(your_email)) {

                $("#your_email").focus();

                $("#errorBox").html("<div class='alert alert-error fade in'> <a href='#' data-dismiss='alert' class='close'>x</a> <strong>Please Enter Your Valid Email Id</strong></div>");


            } else if (f_name == "") {

                $("#friend_name").focus();

                $("#errorBox").html("<div class='alert alert-error fade in'> <a href='#' data-dismiss='alert' class='close'>x</a> <strong>Please Enter Your Friend Name</strong></div>");


            } else if (email == "") {

                $("#friend_email").focus();

                $("#errorBox").html("<div class='alert alert-error fade in'> <a href='#' data-dismiss='alert' class='close'>x</a> <strong>Please Enter Your Friend Valid Email Id</strong></div>");


            } else if (!emailRegex.test(email)) {

                $("#friend_email").focus();

                $("#errorBox").html("<div class='alert alert-error fade in'> <a href='#' data-dismiss='alert' class='close'>x</a> <strong>Please Enter Your Friend Valid Email Id</strong></div>");


            } else {


                var dataString = 'name=' + name + '&f_name=' + f_name + '&your_email=' + your_email + '&email=' + email + '&type_id=' + id;


                $.ajax({

                    type: "POST",

                    url: "<%= base_url %>home/add_tell_to_friend/project",

                    data: dataString,

                    success: function (response) {

                        jQuery('#tell_friend_response').html(response);

                    }

                });


            }

        }


