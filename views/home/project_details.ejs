        function Search_Emi() {

            var amount = $("#amount").val();

            var interest = $("#interest").val();

            var tenure = $("#tenure").val();

            if (!$.isNumeric(amount) || !$.isNumeric(interest) || !$.isNumeric(tenure)) {

                jQuery('#emi_value').html('<h5>Please Enter Numeric Values Only</h5>');

            } else if (amount == null || amount == '' || amount == '0') {

                jQuery('#emi_value').html('<h5>The Amount Value Should Not be 0</h5>');

            } else if (interest == null || interest == '' || interest == '0') {

                jQuery('#emi_value').html('<h5>The Interest Value Should Not be 0</h5>');

            } else if (tenure == null || tenure == '' || tenure == '0') {

                jQuery('#emi_value').html('<h5>The Tenure Value Should Not be 0</h5>');

            } else {

                var dataString = 'amount=' + amount + '&interest=' + interest + '&tenure=' + tenure;

                //alert(dataString);

                $.ajax({

                    type: "POST",

                    url: "<%= base_url %>home/emi_calculate",

                    data: dataString,

                    success: function (response) {

                        jQuery('#emi_value').html(response);

                    }

                });

            }


        }


