    <script type="text/javascript">


        function showAjaxModal(url) {

            // SHOWING AJAX PRELOADER IMAGE

            jQuery('#modal_ajax .modal-body').html('');


            // LOADING THE AJAX MODAL

            jQuery('#modal_ajax').modal('show', {backdrop: 'true'});


            // SHOW AJAX RESPONSE ON REQUEST SUCCESS

            $.ajax({

                url: url,

                success: function (response) {

                    jQuery('#modal_ajax .modal-body').html(response);

                }

            });

        }


        function showAjaxModal1(url) {

            // SHOWING AJAX PRELOADER IMAGE

            jQuery('#modal_ajax1 .modal-body').html('');


            // LOADING THE AJAX MODAL

            jQuery('#modal_ajax1').modal('show', {backdrop: 'true'});


            // SHOW AJAX RESPONSE ON REQUEST SUCCESS

            $.ajax({

                url: url,

                success: function (response) {

                    jQuery('#modal_ajax1 .modal-body').html(response);

                }

            });

        }


        function shedule_site_visit() {

            var name1 = $("#name1").val();

            var phone1 = $("#phone1").val();

            var email1 = $("#email1").val();

            var date = $("#date").val();

            var from_time = $("#from_time").val();

            var from_value = $("#from_value").val();

            var to_time = $("#to_time").val();

            var to_value = $("#to_value").val();


            var dataString = 'name=' + name1 + '&phone=' + phone1 + '&email=' + email1 + '&date=' + date + '&from=' + from_time + ' ' + from_value + '&to=' + to_time + ' ' + to_value;


            $.ajax({

                type: "POST",

                url: "<%= base_url %>home/shedule_site_visit/project",

                data: dataString,

                success: function (response) {

                    jQuery('#shedule_visit_response').html(response);

                }

            });

        }


        function tell_to_friend() {

            var emailRegex = /^[A-Za-z0-9._]*\@[A-Za-z]*\.[A-Za-z]{2,5}$/;

            var filter = /^[0-9-+]+$/;

            var name = $("#your_name").val();

            var f_name = $("#friend_name").val();

            var your_email = $("#your_email").val();

            var email = $("#friend_email").val();

            var id = '<%= project_id %>'

            if (name == "") {

                $("#your_name").focus();

                $("#errorBox").html("<div class='alert alert-error fade in'> <a href='#' data-dismiss='alert' class='close'>x</a> <strong>Please Enter Your Name</strong></div>");


            } else if (your_email == "") {

                $("#your_email").focus();

                $("#errorBox").html("<div class='alert alert-error fade in'> <a href='#' data-dismiss='alert' class='close'>x</a> <strong>Please Enter Your Valid Email Id</strong></div>");


            } else if (!emailRegex.test(your_email)) {

                $("#your_email").focus();

                $("#errorBox").html("<div class='alert alert-error fade in'> <a href='#' data-dismiss='alert' class='close'>x</a> <strong>Please Enter Your Valid Email Id</strong></div>");


            } else if (f_name == "") {

                $("#friend_name").focus();

                $("#errorBox").html("<div class='alert alert-error fade in'> <a href='#' data-dismiss='alert' class='close'>x</a> <strong>Please Enter Your Friend Name</strong></div>");


            } else if (email == "") {

                $("#friend_email").focus();

                $("#errorBox").html("<div class='alert alert-error fade in'> <a href='#' data-dismiss='alert' class='close'>x</a> <strong>Please Enter Your Friend Valid Email Id</strong></div>");


            } else if (!emailRegex.test(email)) {

                $("#friend_email").focus();

                $("#errorBox").html("<div class='alert alert-error fade in'> <a href='#' data-dismiss='alert' class='close'>x</a> <strong>Please Enter Your Friend Valid Email Id</strong></div>");


            } else {


                var dataString = 'name=' + name + '&f_name=' + f_name + '&your_email=' + your_email + '&email=' + email + '&type_id=' + id;


                $.ajax({

                    type: "POST",

                    url: "<%= base_url %>home/add_tell_to_friend/project",

                    data: dataString,

                    success: function (response) {

                        jQuery('#tell_friend_response').html(response);

                    }

                });


            }

        }

        function send_enquiry() {

            var name2 = $("#name2").val();

            var phone2 = $("#phone2").val();

            var email2 = $("#email2").val();

            var comments2 = $("#comments2").val();

            var dataString = 'name=' + name2 + '&phone=' + phone2 + '&comments=' + comments2 + '&email=' + email2;


            $.ajax({

                type: "POST",

                url: "<%= base_url %>home/project_enquiry/<%= project_id %>",

                data: dataString,

                success: function (response) {

                    jQuery('#enquiry_response').html(response);

                }

            });

        }


    </script>


