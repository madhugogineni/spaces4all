<!DOCTYPE HTML>

<html class="no-js">

<head>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">

    <title> Spaces4All - <%=page_title%> </title>

    <meta name="description" content="">

    <meta name="keywords" content="">

    <meta name="author" content="">

    <meta name="viewport"
        content="width=device-width, user-scalable=no, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0">

    <link href="<%=base_url %>stylesheets/bootstrap.css" rel="stylesheet" type="text/css">

    <link href="<%=base_url %>stylesheets/style.css" rel="stylesheet" type="text/css">

    <link href="<%=base_url %>plugins/prettyphoto/css/prettyPhoto.css" rel="stylesheet" type="text/css">

    <link href="<%=base_url %>plugins/owl-carousel/css/owl.carousel.css" rel="stylesheet" type="text/css">

    <link href="<%=base_url %>plugins/owl-carousel/css/owl.theme.css" rel="stylesheet" type="text/css">

    <!--[if lte IE 9]><link rel="stylesheet" type="text/css" href="css/ie.css" media="screen" /><![endif]-->

    <link class="alt" href="<%=base_url %>stylesheets/color.css" rel="stylesheet" type="text/css">

    <script src="http://code.jquery.com/jquery-1.6.3.min.js"></script>

    <script src="<%=base_url %>javascripts/modernizr.js"></script>



    <script>



        function show_sub_type(str) {

            //alert(1);

            if (str == '') {

                $('#bedrooms').attr("disabled", false);

                $('#bathrooms').attr("disabled", false);

            }

            if (str == '1') {

                $('#bedrooms').attr("disabled", true);

                $('#bathrooms').attr("disabled", true);

            }

            if (str == '4') {

                $('#bedrooms').attr("disabled", false);

                $('#bathrooms').attr("disabled", false);

            }
            getPropertySubTypes(str, "#property_sub_type");
        }
        function Show_locality(str) {

            //alert(str);

            if (str.length == 0) {

                document.getElementById("Showlocality").innerHTML = "";

                return;

            }



            if (window.XMLHttpRequest) {// code for IE7+, Firefox, Chrome, Opera, Safari

                xmlhttp = new XMLHttpRequest();

            }

            else {// code for IE6, IE5

                xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");

            }

            xmlhttp.onreadystatechange = function () {

                if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {

                    //alert(xmlhttp.responseText);

                    document.getElementById("Showlocality").innerHTML = xmlhttp.responseText;

                }

            }

            xmlhttp.open("GET", "<?php echo base_url();?>ajax/get_locality/" + str, true);

            xmlhttp.send();

        }



        function hide_bedrooms(str) {

            var value = document.getElementById("property_type").value;

            if (str == '14') {

                $('#bedrooms').attr("disabled", true);

            } else {

                if (value == '1') {

                    $('#bedrooms').attr("disabled", true);

                } else {

                    $('#bedrooms').attr("disabled", false);

                }

            }

        }



    </script>



</head>

<body>

    <div class="body">

        <!-- Start Site Header -->

        <% include includes/header.ejs %>

        <!-- End Site Header -->

        <!-- Site Showcase -->

        <div class="site-showcase">

            <!-- Start Page Header -->

            <div class="parallax page-header">

                <div class="container">

                    <div class="row">

                        <div class="col-md-12">

                            <h1>RENT IN</h1>

                        </div>

                    </div>

                </div>

            </div>

            <!-- End Page Header -->

        </div>

        <!-- Start Content -->

        <div class="main" role="main">

            <div id="content" class="content full">

                <div class="container">

                    <div class="page">

                        <div class="row">

                            <div class="sidebar left-sidebar col-md-3">

                                <div class="property-grid">
                                    <div class="property-block">

                                        <a href="<%=base_url %>home/rent_details/<%=rentDetails.rent_id %>"
                                            class="property-featured-image">

                                            <img src="<%=base_url %>uploads/rent/<%=rentDetails.image %>"
                                                style="height: 170px" alt="Spacel4all">

                                            <span class="images-count"> <?=$city?> </span>

                                            <span class="badges" style="color: #000">RENT</span>

                                        </a>

                                        <div class="property-info" style="line-height: 5px">


                                            <h4><a href="<%=base_url %>home/rent_details/<%=rentDetails.rent_id %>"
                                                    style="text-overflow: ellipsis;width: 100px;overflow: hidden;"><%=rentDetails.property_sub_type_name %></a>
                                            </h4>

                                            <hr><span><%=rentDetails.property_name %></span>
                                            <hr>

                                            <span class="location"><%=rentDetails.locality_name %>,
                                                <%=rentDetails.city_name %></span>
                                            <hr>

                                            <% if(rentDetails.property_sub_type == '14' || rentDetails.property_type == '1'){ %>

                                            <span><%=rentDetails.builtup_area %> Sq.Ft</span>
                                            <hr>

                                            <% }else { %>

                                            <span><%=rentDetails.bedrooms %> BHK</span>
                                            <hr>

                                            <% } %>

                                            <span><%=rentDetails.price %></span>
                                            <hr><br />

                                            <a href="<%=base_url %>home/rent_details/<%=rentDetails.rent_id %>"> <button
                                                    type="button" class="btn btn-danger"> <i
                                                        class="fa fa-angle-double-right"></i>&nbsp; View Details
                                                </button> </a>

                                        </div>

                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="tabs">
                                    <h3 class="widgettitle">RENT IN SEARCH&nbsp;&nbsp;</h3>
                                    <hr>

                                    <div class="">

                                        <div id="" class="lgray-bg padding-as25">

                                            <form action="<%=base_url %>home/rent" method="get">

                                                <div class="row ">

                                                    <div class="col-md-6 col-sm-4">

                                                        <select name="property_type" id="property_type"
                                                            onchange="show_sub_type(this.value)"
                                                            class="form-control margin-0">
                                                            <script>
                                                                setUpPropertyTypes(property_type);
                                                            </script>

                                                        </select>

                                                    </div>

                                                    <div class="col-md-6 col-sm-4">

                                                        <select name="property_sub_type" id="property_sub_type"
                                                            onchange="hide_bedrooms(this.value)"
                                                            class="form-control margin-0">

                                                            <option value="">--Property Sub Type--</option>

                                                        </select>

                                                    </div>

                                                </div>

                                                <br>

                                                <div class="row">



                                                    <div class="col-md-6 col-sm-4">

                                                        <select name="city" id="city"
                                                            onchange="showLocality(this.value,'#Showlocality')"
                                                            class="form-control margin-0">

                                                            <option value="">City</option>
                                                            <script>
                                                                setUpCities("#city");
                                                            </script>

                                                        </select>

                                                    </div>

                                                    <div class="col-md-6 col-sm-4">

                                                        <select name="locality" id="Showlocality"
                                                            class="form-control margin-0">

                                                            <option value="">---Select locality---</option>

                                                        </select>

                                                    </div>

                                                </div>

                                                <br>

                                                <div class="row">



                                                    <div class="col-md-6 col-sm-4">

                                                        <select name="bedrooms" id="bedrooms"
                                                            class="form-control margin-0 ">

                                                            <option value="">Bed Rooms</option>

                                                            <% for( var i = 1; i <= 5; i ++) { %>
                                                            <option value="<%=i %>"><%=i %></option>
                                                            <% } %>

                                                        </select>

                                                    </div>

                                                    <div class="col-md-6 col-sm-4">

                                                        <select name="posted_by" class="form-control margin-0">

                                                            <option value=""> Posted By </option>

                                                            <option value="Owner">Owner</option>

                                                            <option value="Broker">Broker</option>

                                                        </select>

                                                    </div>

                                                </div>

                                                <br>

                                                <div class="row">

                                                    <div class="col-md-6 col-sm-4">

                                                        <select name="min_price" class="form-control margin-0 ">

                                                            <option value="">Min Price</option>
                                                            <% for(var i = 0;i <= 100000; i = i + 5000) { 
                                                               var value;
                                                                if( i == 0) {
                                                                    value = 1000;
                                                                } else {
                                                                    value = i;
                                                                }
                                                                if(value == 100000) {
                                                                    value = parseFloat(value)/100000;
                                                                    value = value + " Lac";
                                                                }                                                                
                                                                %>
                                                            <option value="<%=value%>">Rs <%=value %> </option>

                                                            <% } %>

                                                        </select>

                                                    </div>



                                                    <div class="col-md-6 col-sm-4">

                                                        <select name="max_price" class="form-control margin-0 ">

                                                            <option value="">Max Price</option>

                                                            <% for(var i = 10000 ;i <= 200000;i = i + 5000) {
                                                                var value;
                                                                if( i >= 100000) {
                                                                    value = parseFloat(i)/100000;
                                                                    value = value + " Lac";
                                                                }else {
                                                                    value = i;
                                                                } 
                                                            %>
                                                            <option value="<%=value %>">Rs <%=value %></option>

                                                            <% } %>

                                                            <!-- <?php

															for($mx = 10000; $mx <= 200000; $mx += 5000) {

															if ($mx >= 100000) {

																$max = (( float ) $mx) / 100000;

																$max = 'Rs ' . $max . ' Lac';

															} else {

																$max = 'Rs ' . $mx;

															}

															?>

                                                            <option value="<?=$mx?>"
                                                                <?php if(isset($_GET['max_price'])){if($_GET['max_price']==$mx){echo 'selected';}}?>>
                                                                <?=$max?></option>

                                                            <?php

															}

															?> -->

                                                        </select>

                                                    </div>

                                                </div>

                                                <br>



                                                <div class="row">

                                                    <div class="col-md-6 col-sm-4">

                                                        <input type="submit" name="Search"
                                                            class="btn btn-primary btn-block btn-lg" value="Search">

                                                    </div>

                                                    <div class="col-md-6 col-sm-4"></div>

                                                </div>

                                                <br>



                                            </form>

                                        </div>





                                    </div>

                                </div>



                            </div>

                            <!-- Start Sidebar -->

                            <div class="sidebar right-sidebar col-md-3">

                                <% include includes/services-sidebar.ejs %>

                            </div>

                        </div>

                    </div>

                </div>

            </div>

        </div>

        <!-- Start Site Footer -->

        <% include includes/footer.ejs %>
</body>

</html>