<% include ../includes/header.ejs %>

<!-- Left side column. contains the logo and sidebar -->

<% include ../includes/menu.ejs %>


<!-- Content Wrapper. Contains page content -->

<div class="content-wrapper">

    <!-- Content Header (Page header) -->

    <section class="content-header">

        <h1>

            <%= page_title %>

        </h1>

        <ol class="breadcrumb">

            <li><a href="<%= base_url %>admin"><i class="fa fa-home"></i> Home</a></li>

            <li class="active"><%= page_title %></li>

        </ol>

    </section>


    <!-- Main content -->


    <section class="content">

        <div class="row">

            <div class="col-xs-12">


                <div class="box">

                    <div class="box-header">

                        <div class="col-md-5"></div>

                        <div class="col-md-2">


                        </div>

                    </div>

                    <!-- /.box-header -->

                    <div class="box-body">

                        <table id="example1" class="table table-bordered table-striped">

                            <thead>

                            <tr>

                                <th>S.No</th>

                                <th>Property Name</th>

                                <th>Property Sub Type</th>

                                <th>Locality</th>

                                <th>City</th>

                                <th>Name</th>

                                <th>Email</th>

                                <th>Phone</th>

                                <th>Comments</th>

                                <th>Actions</th>

                            </tr>

                            </thead>

                            <tbody>

                            <% for(var i = 0;i < enquiries.length;i++) {
                                var enquiry = enquiries[i]; %>

                            <tr>

                                <td><%= i %></td>

                                <td><%= enquiry.property_name %></td>

                                <td><%= enquiry.property_sub_type %></td>

                                <td><%= enquiry.city_name %></td>

                                <td><%= enquiry.locality_name %></td>

                                <td><%= enquiry.name %></td>

                                <td><%= enquiry.email %></td>

                                <td><%= enquiry.phone %></td>

                                <td><%= enquiry.comments %></td>

                                <td>

                                    <a href="<%= base_url %>admin/enquiry/rent/delete/<%= enquiry.enquiry_id %>"
                                       onclick="return confirm('Are You Sure Want to Delete ?')"><i
                                                class="fa fa-trash-o"></i></a>

                                </td>

                            </tr>

                            <% } %>

                            </tbody>

                        </table>

                    </div>

                    <!-- /.box-body -->

                </div>

                <!-- /.box -->

            </div>

            <!-- /.col -->

        </div>

        <!-- /.row -->

    </section>

    <!-- /.content -->


</div>

<!-- /.content-wrapper -->


<!-- Modal -->


<!-- (Ajax Modal)-->

<div class="modal fade" id="modal_ajax">

    <div class="modal-dialog">

        <div class="modal-content">

            <form action="<%= base_url %>admin/edit_city" method="post">

                <div class="modal-header">

                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>

                    <h4 class="modal-title">Edit City</h4>

                </div>


                <div class="modal-body">


                </div>


                <div class="modal-footer">

                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>

                    <button type="submit" class="btn btn-primary">Save Changes</button>

                </div>

            </form>

        </div>

    </div>

</div>


<div id="myModal" class="modal fade" role="dialog">

    <div class="modal-dialog">


        <!-- Modal content-->

        <div class="modal-content">

            <div class="modal-header">

                <button type="button" class="close" data-dismiss="modal">&times;</button>

                <h4 class="modal-title">Add City</h4>

            </div>

            <form action="<%= base_url %>admin/add_city" method="post">

                <div class="modal-body">


                    <div class="box-body">

                        <div class="form-group">

                            <label for="city">City</label> <input

                                    class="form-control" id="city" name="city"

                                    placeholder="Enter City" type="text" required="required">

                        </div>

                    </div>

                    <!-- /.box-body -->


                </div>

                <div class="modal-footer">

                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>

                    <button type="submit" class="btn btn-primary">Add</button>

                </div>

            </form>

        </div>


    </div>

</div>


<% include ../includes/footer.ejs %>